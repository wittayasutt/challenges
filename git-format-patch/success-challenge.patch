+	constructor(props) {
+		super()
+
+		this.state = {
+			charities: []
+		}
+	}
+
+	componentDidMount() {
+		fetch('http://localhost:3001/charities')
+			.then(resp => resp.json())
+			.then(data => {
+				this.setState({ charities: data })
+			})
+
+		fetch('http://localhost:3001/payments')
