+
+		return charities.map((item, index) => <Card key={index} item={item} />)
+	}
+}
+
+const mapDispatchToProps = dispatch =>
+	bindActionCreators({ updateTotalDonate }, dispatch)
+
+export default connect(null, mapDispatchToProps)(Cards)
